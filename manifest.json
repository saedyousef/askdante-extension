{
  "manifest_version": 3,
  "name": "AskDante: Total work time calculator",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "action": {
	"default_popup": "popup.html",
	"default_icon": {
	  "16": "icon.png",
	  "48": "icon.png",
	  "128": "icon.png"
	}
  },
  "background": {
	"service_worker": "background.js"
  },
  "content_scripts": [
	{
	  "matches": ["*://*.askdante.com/*"],
	  "js": ["content.js"]
	}
  ]
}
